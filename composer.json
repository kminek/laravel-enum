{
    "name": "kminek/laravel-enum",
    "type": "library",
    "description": "Use myclabs/php-enum as Eloquent model attributes",
    "keywords": ["enum", "laravel"],
    "homepage": "https://github.com/kminek/laravel-enum",
    "license": "MIT",
    "require": {
        "php": ">=8.0.0",
        "illuminate/contracts": "8.*",
        "myclabs/php-enum": "1.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.*",
        "illuminate/database": "8.*",
        "illuminate/events": "8.*",
        "symfony/var-dumper": "5.*",
        "phpunit/phpunit": "9.*"
    },
    "autoload": {
        "psr-4": {
            "Kminek\\LaravelEnum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "cs": [
            "php-cs-fixer fix"
        ],
        "test": [
            "phpunit"
        ]
    }
}
